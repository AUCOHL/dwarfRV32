puts stdout "Sourcing design .magicrc for technology EFXH035B ..."

# drc off
drc euclidean on

set PDKPATH "/ef/tech/XFAB/EFXH035B"

# loading technology
tech load $PDKPATH/libs.tech/magic/current/EFXH035B.tech

# load device generator
source $PDKPATH/libs.tech/magic/current/EFXH035B.tcl

# load bind keys
source $PDKPATH/libs.tech/magic/current/EFXH035B-BindKeys

# set units to user grid
snap grid

# add path to reference cells
set MAGPATH "${PDKPATH}/libs.ref/maglef"
addpath ${MAGPATH}/primdev
addpath ${MAGPATH}/D_CELLS
addpath ${MAGPATH}/A_CELLS
addpath ${MAGPATH}/IO_CELLS
addpath ${MAGPATH}/IO_CELLS_F
addpath ${MAGPATH}/IO_CELLS_MV
addpath ${MAGPATH}/IO_CELLS_MVF

# add path to GDS cells

# add path to IP from catalog.  This procedure defined in the PDK script.
catch {magic::query_mylib_ip}
